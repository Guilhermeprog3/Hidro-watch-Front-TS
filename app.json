{
  "expo": {
    "name": "Hidro-Watch",
    "slug": "Hidro-Watch-Front",
    "version": "1.0.0",
    "projectId": "ec65b7e2-cebf-42d1-8d45-2ed2f00f334c",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1128",
      "dark": {
        "backgroundColor": "#0A1128"
      }
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A1128",
        "dark": {
          "backgroundColor": "#0A1128"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1128"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A1128",
        "dark": {
          "backgroundColor": "#0A1128"
        }
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.guilhermeriosdev.HidroWatchFront"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f623d0cc-e0aa-4f56-9769-869b386c0540"
      }
    }
  }
}
